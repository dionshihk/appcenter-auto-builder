{
    "name": "appcenter-auto-builder",
    "version": "1.3.2",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "author": "Dion Shi",
    "license": "MIT",
    "keywords": [
        "microsoft appcenter",
        "appcenter builder",
        "ios app builder",
        "android app builder",
        "mobile app builder",
        "react native builder"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dionshihk/appcenter-auto-builder"
    },
    "scripts": {
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
        "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
        "build": "pnpm format-check && pnpm build:fast",
        "build:fast": "rm -rf ./lib && tsc --composite false",
        "prepublishOnly": "pnpm build:fast"
    },
    "dependencies": {
        "axios": "1.12.0",
        "fs-extra": "11.3.0",
        "unzipper": "0.12.3"
    },
    "devDependencies": {
        "@types/fs-extra": "11.0.4",
        "@types/node": "24.1.0",
        "@types/unzipper": "0.10.11",
        "prettier": "3.6.2",
        "ts-node": "10.9.2",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "pnpm@10.13.1"
}
